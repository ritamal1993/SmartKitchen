@model IEnumerable<SmartKitchen3.Models.Calendar>

@{
    ViewData["Title"] = "UserSelectCalendars";
}


<div>    @Html.ListBox("Tags", (MultiSelectList)ViewBag.MultiSelectTags)

    <select class="selectpicker" asp-items="@ViewBag.Calendars" multiple data-live-search="true" data-selected-text-format="count" title="Choose Calenders">
    </select>
    <label class="mdb-main-label">Label example</label>
    <button class="btn-save btn btn-primary btn-sm">Save</button>
</div>


@section Scripts {
    @{ await Html.RenderPartialAsync("_ValidationScriptsPartial");}




    <!-- Latest compiled and minified JavaScript -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap-select@1.13.9/dist/js/bootstrap-select.min.js"></script>

    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-select@1.13.9/dist/css/bootstrap-select.min.css">


}

