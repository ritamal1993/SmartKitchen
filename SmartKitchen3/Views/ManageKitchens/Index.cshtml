@model IEnumerable<SmartKitchen3.Models.Kitchen>
@using SmartKitchen3.Models
@{
    ViewBag.Title = "Index";
}
@section Scripts{
    <script type="text/javascript" src="~/Scripts/SearchTable.js"></script>
}

<!doctype html>
<html>
@{
    ViewData["Title"] = "Your food?  ";
}
<head>
    <link rel="stylesheet" href="6.css" type="text/css" media="screen" />
</head>
<body>
    <header>
        <h2>Kitchen Location</h2>
    </header>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.4.1.min.js"></script>
    <style>

        /* Always set the map height explicitly to define the size of the div
        * element that contains the map. */
        #map {
            height: 500px;
            width: 1000px;
        }
        /* Optional: Makes the sample page fill the window. */
        html, body {
            height: 15%;
            margin: 15;
            padding: 15;
        }
    </style>
    <div id="map"></div>
    <script>
        var map;
        function initMap() {
            var location = { lat: 20, lng: 20 };
            map = new google.maps.Map(document.getElementById('map'), {
                center: location,
                zoom: 2,
                disableDefaultUI: true

            });
        }
        function addMarker(coords) {
            var marker = new google.maps.Marker({
                position: coords,
                map: map,
                zoom: 3,
                disableDefaultUI: true,
                center: coords
            });
        }</script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA6sPvCq7FsVoGBs8rijuRQghDxEu28V_0&callback=initMap"
            async defer></script>



    <link rel="stylesheet" href="~/Content/Design.css" />
    <h2>Manage Kitchens</h2>




    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <input class="form-control" id="searchable" type="text" placeholder="Search..">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.LocationLatitudes)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.LocationLongitude)
                </th>
                <th>Location</th>
                <th></th>
                <th></th>
            </tr>
        </thead>
        <tbody id="search-table">
            @foreach (var item in Model)
            {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LocationLatitudes)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LocationLongitude)
                </td>

                <td>
                    <button type="button" onclick="addMarker({ lat: @item.LocationLatitudes, lng: @item.LocationLongitude })">Show on map</button>
                </td>
                

                <td>
                    @Html.Partial("_TableButtonPartial", new IndividualButtonPartial { KitchenId= item.KitchenId })
                </td>


                
            </tr>
            }
        </tbody>
    </table>
