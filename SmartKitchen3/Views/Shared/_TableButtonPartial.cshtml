@model SmartKitchen3.Models.IndividualButtonPartial
    @using SmartKitchen3.Models


<td style="width:400px;">
    <div
        class="btn-group"role="group">
  
        @Html.Partial("_IndividualButtonPartial",
    new IndividualButtonPartial
    {
        Action = "Edit",
        ButtonType = "btn-primary",
        Glyph = "pencil",
        Text = "Edit Button",
        CategoryId = Model.CategoryId,
        RecipeId=Model.RecipeId,
        UserId=Model.UserId,
        CalendarId=Model.CalendarId,
        IngredientId=Model.IngredientId,
        KitchenId=Model.KitchenId,
       

    })

        @Html.Partial("_IndividualButtonPartial",
    new IndividualButtonPartial
    {
        Action = "Details",
        ButtonType = "btn-success",
        Glyph = "list",
        Text = "Details Button",
        CategoryId = Model.CategoryId,
        RecipeId = Model.RecipeId,
        UserId = Model.UserId,
        CalendarId = Model.CalendarId,
        IngredientId = Model.IngredientId,
        KitchenId = Model.KitchenId,
       
    })
        @Html.Partial("_IndividualButtonPartial",
    new IndividualButtonPartial
    {
        Action = "Delete",
        ButtonType = "btn-danger",
        Glyph = "trash",
        Text = "Delete Button",
        CategoryId = Model.CategoryId,
        RecipeId = Model.RecipeId,
        UserId = Model.UserId,
        CalendarId = Model.CalendarId,
        IngredientId = Model.IngredientId,
        KitchenId = Model.KitchenId,
      
    })

        

    </div>
</td>




